        function SaveData() {
            // Serialize form data into an object
            let formData = $('#employeeForm').serializeArray();
            let employeeData = {};
            formData.forEach(field => {
                employeeData[field.name] = field.value;
            });

            // Collect Educational Info
            let eInstituteId = [];
            let ePassingYear = [];
            let eBatchNo = [];
            let eGrade = [];
            let eDegreeName = [];
            let eAchievement = [];
            let eCourseDuration = [];

            $('#EduTbl tbody tr').each(function () {
                let row = $(this);
                eInstituteId.push(row.find('td:eq(0)').data('id')); // Assuming there's an ID for the institute
                ePassingYear.push(row.find('td:eq(1)').text());
                eBatchNo.push(row.find('td:eq(2)').text());
                eGrade.push(row.find('td:eq(3)').text());
                eDegreeName.push(row.find('td:eq(4)').text());
                eAchievement.push(row.find('td:eq(5)').text());
                eCourseDuration.push(row.find('td:eq(6)').text());
            });

            // Collect Institute Info
            let instituteId = [];
            let passingYear = [];
            let batchNo = [];
            let grade = [];
            let degreeName = [];
            let achievement = [];
            let courseDuration = [];

            $('#TrainingTbl tbody tr').each(function () {
                let row = $(this);
                instituteId.push(row.find('td:eq(0)').data('id')); // Assuming there's an ID for the institute
                passingYear.push(row.find('td:eq(1)').text());
                batchNo.push(row.find('td:eq(2)').text());
                grade.push(row.find('td:eq(3)').text());
                degreeName.push(row.find('td:eq(4)').text());
                achievement.push(row.find('td:eq(5)').text());
                courseDuration.push(row.find('td:eq(6)').text());
            });

            // Collect Posting Info
            let rankId = [];
            let postingFrom = [];
            let postingTo = [];
            let rangeId = [];
            let districtId = [];
            let zoneId = [];
            let thanaId = [];
            let pReMarks = [];
            let promotionDate = [];

            $('#PostingTbl tbody tr').each(function () {
                let row = $(this);
                rankId.push(row.find('td:eq(0)').data('id')); // Assuming there's an ID for the rank
                postingFrom.push(row.find('td:eq(1)').text());
                postingTo.push(row.find('td:eq(2)').text());
                rangeId.push(row.find('td:eq(3)').data('id'));
                districtId.push(row.find('td:eq(4)').data('id'));
                zoneId.push(row.find('td:eq(5)').data('id'));
                thanaId.push(row.find('td:eq(6)').data('id'));
                promotionDate.push(row.find('td:eq(7)').text());
                pReMarks.push(row.find('td:eq(8)').text());
            });

            // Collect Address Info
            let addressType = [];
            let roadInfo = [];
            let aVillegeId = [];
            let aUnionId = [];
            let aThanaId = [];
            let aDistrictId = [];
            let aDivisionId = [];

            $('#AddressTbl tbody tr').each(function () {
                let row = $(this);
                addressType.push(row.find('td:eq(0)').data('id')); // Assuming there's an ID for the address type
                aDivisionId.push(row.find('td:eq(1)').data('id'));
                aDistrictId.push(row.find('td:eq(2)').data('id'));
                aThanaId.push(row.find('td:eq(3)').data('id'));
                aUnionId.push(row.find('td:eq(4)').data('id'));
                aVillegeId.push(row.find('td:eq(5)').data('id'));
                roadInfo.push(row.find('td:eq(6)').text());
            });

            // Prepare the final object
            let combinedData = {
                ...employeeData,
                eInstituteId,
                ePassingYear,
                eBatchNo,
                eGrade,
                eDegreeName,
                eAchievement,
                eCourseDuration,
                instituteId,
                passingYear,
                batchNo,
                grade,
                degreeName,
                achievement,
                courseDuration,
                rankId,
                postingFrom,
                postingTo,
                rangeId,
                districtId,
                zoneId,
                thanaId,
                promotionDate,
                pReMarks,
                addressType,
                roadInfo,
                aVillegeId,
                aUnionId,
                aThanaId,
                aDistrictId,
                aDivisionId
            };

            // Confirmation dialog
            swal({
                title: 'Are you sure?',
                text: 'Do you want to save this data?',
                type: 'warning',
                showCancelButton: true,
                confirmButtonColor: '#3085d6',
                cancelButtonColor: '#d33',
                confirmButtonText: 'Yes, Save It!'
            }).then(function (res) {
                if (res) {
                    // AJAX request
                    $.ajax({
                        url: "/EmployeeArea/EmployeInfo/CreateEmployeeProfile",
                        type: "POST",
                        contentType: "application/json", // Send as JSON
                        data: JSON.stringify(combinedData),
                        success: function (response) {
                            if (response != 0) {
                                swal('', 'Save Successfully!', 'success');
                                location.reload(true);
                            } else {
                                swal('', 'Invalid Attempt!', 'warning');
                                return false;
                            }
                        },
                        error: function (error) {
                            console.error('Error saving data:', error);
                            swal('', 'An error occurred while saving data.', 'error');
                        }
                    });
                }
            }, function (dismiss) {
                if (dismiss === 'cancel' || dismiss === 'close') {
                    return false;
                }
            });
        }

    </script>
