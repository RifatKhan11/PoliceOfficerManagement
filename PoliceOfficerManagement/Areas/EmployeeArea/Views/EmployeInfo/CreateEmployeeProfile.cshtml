        function addAddress() {
            // Get values from the form
            const addressType = $('#addressType').val();
            const division = $('#divisionId').val();  // Keep values (not text) for input fields
            const district = $('#districtId').val();
            const thana = $('#thanaId').val();
            const union = $('#unionId').val();
            const villege = $('#villegeId').val();
            const roadInfo = $('#roadInfo').val();

            // Validate required fields
            if (!addressType || !division || !district || !thana || !union || !villege || !roadInfo) {
                alert('Please fill all required fields.');
                return;
            }

            // Create new row with input fields
            const newRow = `
                <tr>
                    <td><input type="text" name="addressType[]" value="${addressType}" class="form-control" /></td>
                    <td>
                        <select name="aDivisionId[]" class="form-control">
                            <option value="${division}" selected>${$('#divisionId option:selected').text()}</option>
                        </select>
                    </td>
                    <td>
                        <select name="aDistrictId[]" class="form-control">
                            <option value="${district}" selected>${$('#districtId option:selected').text()}</option>
                        </select>
                    </td>
                    <td>
                        <select name="aThanaId[]" class="form-control">
                            <option value="${thana}" selected>${$('#thanaId option:selected').text()}</option>
                        </select>
                    </td>
                    <td>
                        <select name="aUnionId[]" class="form-control">
                            <option value="${union}" selected>${$('#unionId option:selected').text()}</option>
                        </select>
                    </td>
                    <td><input type="text" name="aVillegeId[]" value="${villege}" class="form-control" /></td>
                    <td><input type="text" name="roadInfo[]" value="${roadInfo}" class="form-control" /></td>
                    <td>
                        <button type="button" class="btn btn-danger btn-sm remove-Address">Remove</button>
                    </td>
                </tr>
            `;

            // Append the new row to the table body
            $('#AddressTbl tbody').append(newRow);

            // Clear the form fields after adding
            $('#addressType').val('');
            $('#divisionId').val('');
            $('#districtId').val('');
            $('#thanaId').val('');
            $('#unionId').val('');
            $('#villegeId').val('');
            $('#roadInfo').val('');
        }



